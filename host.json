{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Warning",
      "Host.Results": "Warning",
      "Function": "Warning",
      "Host.Aggregator": "Warning",
      "azure.storage": "Warning",
      "Azure": "Warning",
      "azure": "Warning",
      "azure.storage.common.storageclient": "Warning"
      }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  }
}